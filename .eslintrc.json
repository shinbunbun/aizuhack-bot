{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "import/no-import-module-exports": "off",
        "import/extensions": [
            "error",
            "always",
            {
                "ignorePackages": true
            }
        ]
    }
}